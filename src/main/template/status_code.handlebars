<td width='20%' class='code'>{{{escape code}}}</td>
<td class="">
	{{{message}}}
	{{#if headers}}
	<table class="header_response">
		<thead>
			<tr>
				<th data-sw-translate>Header</th>
				<th data-sw-translate>Description</th>
				<th data-sw-translate>Type</th>
			</tr>
		</thead>
    <tbody>
      {{#each headers}}
      <tr>
        <td>{{@key}}</td>
        <td>{{{sanitize this.description}}}</td>
        <td>{{{escape this.type}}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
  {{/if}}
</td>
