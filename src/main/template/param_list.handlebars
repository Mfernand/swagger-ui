<td class='code{{#if required}} required{{/if}}'>
	<label for='{{{escape valueId}}}'>{{{sanitize name}}}</label>{{#if required}}<span style="color: red;">&nbsp;*</span>{{/if}}
	{{#if type}}<div class="paramType">{{{escape type}}}</div>{{/if}}
	<div class="paramIn">({{{escape paramType}}})</div>
</td>
